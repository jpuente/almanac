with "ephemeris.gpr";

project Ephemeris_Test is

   for Languages use ("Ada");
   for Source_Dirs use ("test");
   for Object_Dir use "test";
   for Exec_Dir use ".";
   for Main use ("test_ephemeris.ads");

   package Binder is
      for Default_Switches ("ada") use ("-E", "-static");
   end Binder;

   package Compiler is
      for Default_Switches ("ada") use ("-gnat05", "-gnatf", "-g");
   end Compiler;

   package Linker is
      for Default_Switches ("ada") use ("-g");
   end Linker;

   package Builder is
      for Default_Switches ("ada") use ("-g", "-gnatQ");
   end Builder;

end Ephemeris_Test;

