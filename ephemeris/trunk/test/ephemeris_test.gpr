with "../library/ephemeris_library.gpr";

project Ephemeris_Test is

   for Languages use ("Ada");
   for Main use ("test_ephemeris.ads");
   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";

   package Compiler is
      for Default_Switches ("ada") use
        ("-gnat12", "-g", "-gnatQ", "-O1", "-gnatf", "-gnato", "-gnatwa.Xe", "-gnaty");
   end Compiler;

   package Binder is
      for Default_Switches ("ada") use ("-E", "-static");
   end Binder;

   package Linker is
      for Default_Switches ("ada") use ("-g");
   end Linker;

   package Builder is
      for Default_Switches ("ada") use ("-g", "-gnatQ");
   end Builder;

end Ephemeris_Test;

